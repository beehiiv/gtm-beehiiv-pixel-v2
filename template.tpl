const log = require('logToConsole');
const makeInteger = require('makeInteger');
const Object = require('Object');
const createArgumentsQueue = require('createArgumentsQueue');
const injectScript = require('injectScript');
const callInWindow = require('callInWindow');
const copyFromWindow = require('copyFromWindow');

const VERSION = 'v2.0.1';

log('beehiiv pixel loaded', VERSION);
// Constants
const APIARY_ENDPOINT = 'https://ingestion.apiary.beehiiv.net/api/v1/ingestion/pixel';
const PIXEL_SCRIPT_URL = 'https://s3.amazonaws.com/beehiiv-adnetwork-production/pixel-v2.js';
const EXCLUDED_DOMAINS = ['beehiiv.com', 'staginghiiv.com', 'localhiiv.com'];

let bhpxQueue = createArgumentsQueue('bhpx', 'dataLayer'); // Rename to avoid confusion
let queue = []; // Pre-load queue
let isInitialized = false;

// Get event data
const eventData = data;

const makeStringArray = (values) => {
  if (!values) return undefined;
  return values.split(',').map((s) => s.trim());
};

const makeBoolean = (value) => {
  return (value || '').toString() === 'true';
};

const advertiserId = eventData.advertiserId;

// Prepare the event data payload
const payload = {
  data: {
    currency: eventData.currency,
    value_cents: eventData.value_cents ? makeInteger(eventData.value_cents) : undefined,
    num_items: eventData.num_items ? makeInteger(eventData.num_items) : undefined,
    predicted_ltv_cents: eventData.predicted_ltv_cents ? makeInteger(eventData.predicted_ltv_cents) : undefined,
    content_category: eventData.content_category,
    content_ids: makeStringArray(eventData.content_ids),
    content_name: eventData.content_name,
    content_type: eventData.content_type,
    search_string: eventData.search_string,
    status: makeBoolean(eventData.status),
  },
};

// Clean up undefined values
const cleanData = {};
const keys = Object.keys(payload.data);
for (let i = 0; i < keys.length; i++) {
  const key = keys[i];
  if (payload.data[key] !== undefined) {
    cleanData[key] = payload.data[key];
  }
}
payload.data = cleanData;

const onSuccess = () => {
  log('pixel-v2.js loaded');

  // Get the bhpx function from the window object
  const bhpx = copyFromWindow('bhpx');
  log('onSuccess - bhpx:', bhpx);

  if (typeof bhpx !== 'function') {
    log('Error: bhpx is not a function yet');
    data.gtmOnFailure();
    return;
  }

  // Initialize the pixel
  log('calling init', advertiserId, queue.length);
  bhpx.callMethod('init', advertiserId); // Direct function call
  isInitialized = true;
  log('initialized');

  // Process the queue
  if (queue.length > 0) {
    log('processing queue: length =', queue.length);
    for (let i = 0; i < queue.length; i++) {
      const entry = queue[i];
      log('#' + (i + 1) + ' event ' + entry.event);
      bhpx.callMethod(entry.command, entry.event, entry.options); // Direct function call
    }
    queue = []; // Clear queue
  }

  // Handle the current event if initialized
  if (eventData.eventName) {
    log('Sending event', eventData.eventName, payload);
    bhpx.callMethod('track', eventData.eventName, payload);
  }

  data.gtmOnSuccess();
};

const onFailure = () => {
  log('Failed to load pixel-v2.js');
  data.gtmOnFailure();
};

// Push events to the queue if not initialized
if (!isInitialized && eventData.eventName) {
  queue.push({
    command: 'track',
    event: eventData.eventName,
    options: payload,
  });
}

// Inject the script
injectScript(PIXEL_SCRIPT_URL, onSuccess, onFailure);


